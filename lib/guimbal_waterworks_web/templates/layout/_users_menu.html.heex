<ul>
  <%= if @current_users do %>
    <li><%= @current_users.username %></li>
    <li><%= link "Log out", to: Routes.users_session_path(@conn, :delete), method: :delete %></li>
  <% else %>
    <li><%= link "Register", to: Routes.users_registration_path(@conn, :new) %></li>
    <li><%= link "Log in", to: Routes.users_session_path(@conn, :new) %></li>
  <% end %>
</ul>
